{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-param-reassign": [
            "error", {
                "props": true,
                "ignorePropertyModificationsFor": ["element", "box"]
            }
        ],
        "import/prefer-default-export": 0,
        "no-underscore-dangle": [
            "error", {
                "allowAfterThis": true
            }
        ]
    }
}
